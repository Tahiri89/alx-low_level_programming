#include "variadic_functions.h"
#include <stdio.h>
#include <stdlib.h>
#include <stdarg.h>

void print_numbers(const char *separator, const unsigned int n, ...)
{
	va_list argprint;
	unsigned int i;

	va_start(argprint, n);

	for (i = 0; i < n - 1; i++)
	{
		printf("%d %s",va_arg(argprint, int), separator);
		if (i == (n - 1))
		{
			printf("%d\n", va_arg(argprint, int));
		}
	}
	va_end(argprint);
}
